<h1> New Post </h2>

<%= form_for @post do |form| %>

  <%= render partial: "form_errors/errors", locals: {object: @post} %>

  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :content %>
    <%= form.text_area :content %>
  </div>

  <div>
    <%= form.submit "Post" %>
  </div>
<% end %>
